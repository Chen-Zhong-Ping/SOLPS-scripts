cd $WORK/solps/solps5.1/ ; source setup.ksh ; cd $SOLPSTOP/runs/NSTXU/SN_X_Dec2015/07JUL18_S/puff-03e21/Q1.5e6-hc0.5/BCC13-dna0.15 ; $SOLPSTOP/bin/common/b2run b2mn < input.dat >& /dev/null ; if [ -e b2fstate ] && [ `stat -c %s b2fstate` -ge `stat -c %s b2fstati` ] ; then rm -rf b2mn.exe.dir ; sleep 10 ; cp b2fstate b2fstati ; sed -i "\|\$SOPLSTOP/runs/NSTXU/SN_X_Dec2015/07JUL18_S/puff-03e21/Q1.5e6-hc0.5/BCC13-dna0.15|d" $SOLPSTOP/runs/pylauncher/incomplete_jobs.txt ; fi

